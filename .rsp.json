{
  "port": 3000,
  "buildDirectory": "./dist",
  "engine": {
    "launchOptions": {
      "args": [
        "--no-sandbox",
        "--disable-setuid-sandbox"
      ],
      "product": "chrome",
      "headless": true,
      "ignoreHTTPSErrors": true
    },
    "gotoOptions": {
      "timeout": 0
    }
  },
  "routes": [
    "/",
    "/browser/aws",
    "/browser/azure",
    "/browser/google",
    "/blog"
  ]
}
